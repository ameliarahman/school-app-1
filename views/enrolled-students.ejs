<% include partials/head %>

    <body>
        <header>
            <% include partials/menu%>
                <h3>Students Data</h3>
        </header>
        <table border="1" cellpadding="0" cellspacing="0" width="65%">
            <tr>
                <th>No</th>
                <th>ID</th>
                <th>Students Name</th>
                <th>Score</th>
                <th>Desc</th>
            </tr>
            <% dataStudentSubject.forEach(function(row, index){%>
                <tr>
                    <td>
                        <%=index+1%>
                    </td>
                    <td>
                        <%=row.id %>
                    </td>
                    <td>
                        <%=row.Student.getFullName()%>
                    </td>
                    <%if(row.score){ %>
                        <td>
                            <%=row.score%>
                        </td>
                        <td>

                        </td>
                        <%}else{%>
                            <td>
                                <a href="/subjects/<%=row.idSubject%>/<%=row.id%>/givescore">Give Score</a>
                            </td>
                            <td>
                                Empty
                            </td>
                </tr>
                <%}})%>

        </table>
        </div>

    </body>

    </html>